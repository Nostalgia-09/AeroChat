{
	"extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
	"include": [
		"electron.vite.config.*",
		"src/main/**/*.ts",
		"src/preload/*",
		"src/shared/**/*.ts"
	],
	"compilerOptions": {
		"composite": true,
		"types": ["electron-vite/node"],
		"noUnusedLocals": false,
		"noUnusedParameters": false
	}
}
